{
  "name": "eliza",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/eliza/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsup",
        "cwd": "apps/eliza"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --write ./src",
        "cwd": "apps/eliza"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "apps/eliza"
      }
    }
  },
  "tags": []
}
