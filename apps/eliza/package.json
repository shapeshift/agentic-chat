  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizaos/cli": "1.0.0-beta.34",
    "@elizaos/core": "1.0.0-beta.34",
    "@elizaos/plugin-anthropic": "1.0.0-beta.34",
    "@elizaos/plugin-bootstrap": "1.0.0-beta.37",
    "@elizaos/plugin-evm": "1.0.0-beta.34",
    "@elizaos/plugin-openai": "1.0.0-beta.34",
    "@elizaos/plugin-sql": "1.0.0-beta.34",
    "@elizaos/plugin-venice": "1.0.0-beta.28",
    "@langchain/core": "0.3.47",
    "@types/pg": "8.11.10",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.1",
    "pg": "^8.13.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "2.1.5",
